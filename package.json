{
  "name": "hatchways-live-interviewing",
  "displayName": "hatchways-live-interviewing",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hatchways-sidebar-view",
          "title": "Settings",
          "icon": "media/hatchways.svg"
        }
      ]
    },
    "views": {
      "hatchways-sidebar-view": [
        {
          "type": "webview",
          "id": "hatchways-sidebar",
          "name": "hatchways",
          "icon": "media/hatchways.svg",
          "contextualTitle": "Hatchways Settings"
        }
      ]
    },
    "commands": [
      {
        "command": "hatchways-live-interviewing.welcome",
        "title": "Hatchways: Welcome"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w --bundleConfigAsCjs\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "concurrently": "^8.2.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
